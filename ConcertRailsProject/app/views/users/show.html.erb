<h1>Profile Page for <%= @user.name %> </h1>
<h2>Username: <%= @user.username %></h2>
<h2>Age: <%= @user.age %></h2>

<h3>Favorite Artists:<h3>
<ul>
<% @user.artists.each do |a| %>
    <li> 
    <%= link_to a.name, artist_paths(a) %> 
    </li>
    <% end %>
</ul>

<h3>Favorite Venues:<h3>
<ul>
<% @user.venues.each do |v| %>
    <li> 
    <%= link_to v.name, venues_paths(v) %> 
    </li>
    <% end %>
</ul>

<h3>Upcoming Concerts:<h3>
<ul>
<% @user.concerts.each do |c| %>
    <li> 
    <%= link_to c.name, concerts_paths(c) %> 
    </li>
    <% end %>
</ul>