<h1>Create a user</h1>
<%= form_for @user do |f| %>
<%= f.label :name, "Name" %>
<%= f.text_field :name %>
<%= f.label :age, "Age" %>
<%= f.number_field :age %>
<%= f.label :username, "Username" %>
<%= f.text_field :username %>
<%= f.label :password, "Password" %>
<%= f.password_field :password %>
<%= f.submit "Create Profile" %>
<% end %>